#!/usr/bin/env bash

# Get the PID of the current script
script_pid=$$

# Get the PID of the bash processes
bash_pids=$(pgrep -f bash)

# Iterate through each bash process PID
for pid in $bash_pids; do
    # Skip the script's own PID
    if [ $pid -eq $script_pid ]; then
        continue
    fi
    
    # Get the process name
    process_name=$(cat /proc/$pid/cmdline | tr '\0' ' ' | awk '{print $1}')
    
    # Print PID and process name
    echo "$pid $process_name"
done
